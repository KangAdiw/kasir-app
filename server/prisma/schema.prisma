generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"  // <-- Di Versi 5 ini WAJIB ada di sini
}

model Product {
  id        Int      @id @default(autoincrement())
  name      String
  price     Int
  image     String
  category  String
  createdAt DateTime @default(now())
}

model Transaction {
  id          Int      @id @default(autoincrement())
  totalAmount Int
  paidAmount  Int
  change      Int
  items       String
  createdAt   DateTime @default(now())
}